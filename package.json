{
  "name": "mx-avc-plugin",
  "version": "1.0.0",
  "description": "为 AutoVoiceCollation 开发的 Obsidian 插件，提供语音自动整理功能。",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "copy-dist-to-vault": "powershell -NoProfile -Command \"New-Item -ItemType Directory -Force -Path 'D:\\doc\\Obsidian Vault\\avc\\.obsidian\\plugins\\mx-avc-plugin' >$null; Copy-Item -Path 'main.js','manifest.json','styles.css' -Destination 'D:\\doc\\Obsidian Vault\\avc\\.obsidian\\plugins\\mx-avc-plugin' -Force\"",
    "build-and-copy": "npm run build && npm run copy-dist-to-vault"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}
